<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Authors>Efthymios Koktsidis</Authors>
        <Company>Efthymios Koktsidis</Company>
        <Copyright></Copyright>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>Collection of extensions, utilities and interfaces.</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/efthymios-ks/CoreSharp</PackageProjectUrl>
        <RepositoryUrl>https://github.com/efthymios-ks/CoreSharp</RepositoryUrl>
        <PackageTags>Utilities, Extensions, Interfaces</PackageTags>
        <PackageIcon>icon.png</PackageIcon>
        <RepositoryType>git</RepositoryType>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <Title>CoreSharp</Title>
        <Version>1.0.6</Version>
        <PackageReleaseNotes>-Code clean-up.
-New Stream and generic extensions.</PackageReleaseNotes>
        <AssemblyVersion>1.0.6.0</AssemblyVersion>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
        <DocumentationFile></DocumentationFile>
        <NoWarn>1701;1702;CS1573;CS1591;</NoWarn>
        <WarningsAsErrors>NU1605;</WarningsAsErrors>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.10" />
        <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="5.0.10" />
        <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="5.0.10" />
        <PackageReference Include="Microsoft.Extensions.Localization.Abstractions" Version="5.0.10" />
        <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
        <PackageReference Include="System.Data.SqlClient" Version="4.8.3" />
        <PackageReference Include="System.IO.Ports" Version="5.0.1" />
    </ItemGroup>

    <ItemGroup>
        <None Include="..\icon.png">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
    </ItemGroup>

</Project>
